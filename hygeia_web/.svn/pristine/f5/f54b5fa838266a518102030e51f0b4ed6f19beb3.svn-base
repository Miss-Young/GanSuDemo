<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=GBK" />
    <title>登录</title>
    <style type="text/css">
*{margin:0;overflow:hidden;padding:0}
body{background:url(images/yb_03.jpg) repeat-x ;background-position:top;font-size:12px;background-color:#039;font:12px "Microsoft YaHei";height:100%;margin:0 auto;width:100%}
#main{width:1004px;height:600px;margin:0 auto;overflow:hidden; background:url(images/yb_05.jpg) no-repeat; background-position:center top;}
#top{height:50px;padding-top:95px; width:700px; margin:0 auto;}
#logo{margin:0 auto;;padding-top: 15px;font-size:24px;font-weight:bold;color:#fff; text-align:center;letter-spacing:1px;}
#logoImg{}
#systemname{}
#city{height:305px; width:1004px; padding:0;}
#pic{float:left;margin-top:20px; float:left;}
#login{float:left;width:290px;height:305px;color:#fff;padding-left:0px}
#tab{margin-top:45px}
#footer{width:300px;margin:0 auto;text-align:center;color:#fff;font-size:12px; margin-top:10px;}
#center{margin:0 auto;width:860px;height:auto}
.ButtonOK{background:url(images/loginbnt.png) no-repeat center bottom;border:none;cursor:pointer;font-size:0px;height:32px;padding:0;width:125px}
.row{height:35px;line-height:35px;vertical-align:middle;}
.ctrl{}
.text{font-weight:bold;padding-right:4px;text-align:right}
.loginFailed{color:#AD0E25;text-align:center;}
.textinput{ height:28px; width:170px; border:1px solid #6390c2; padding:0px 0 0 3px; font-size:16px; font-weight:bold; background-color:#ffffff; line-height:28px; margin-left:5px;}
#verifyCode{vertical-align:middle}
#verifyImage{cursor:pointer;vertical-align:middle;}
</style>
    <script type="text/javascript">
        document.oncontextmenu = function () { return false; };
        window.onload = function () {
            var footer = document.getElementById("footer");
            if (footer) {
                footer.innerHTML = "Copyright &copy; " + new Date().getFullYear() + " PowerSI. All Rights Reserved.";
            }
            
            //处理自动登录
            var action = getQueryString("action");
            if(action != null && action == "autologin"){
            	var loginUser = getQueryString("loginuser");
            	if(loginUser != null){
            		document.getElementById("UserName").value = loginUser;
            	}
            	document.getElementById("auto_login").value = 'true';
            	document.getElementById("UserPassword").value = '';
                document.getElementById("ButtonOK").click();
            }
        };
        
        function getQueryString(name) {
        	var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)","i");
        	var r = window.location.search.substr(1).match(reg);
        	if (r!=null) return unescape(r[2]); return null;
        }
        
        function keyEnter(type){
        	try{
        		var e = event ? event : (window.event ? window.event : null); 
            	if(e != null && e.keyCode == 13){ 
            		if(type == "user"){
            			document.getElementById("UserPassword").focus();
            		} else if(type == "pwd"){
            			document.getElementById("ButtonOK").click();
            		}
            		
            		event.keyCode = 0;
                    event.returnValue = false;
            	} 
        	} catch(e){}
        }
    </script>
</head>
<body>
    <div id="main">
        <div id="top">
            <div id="logo">
                <span id="systemname">广州市医疗保险管理信息系统</span>
            </div>
        </div>
        <div id="city">
            <div id="center">
                <div id="pic">
                    <img src="images/yb_08.jpg" width="562" height="273" alt="" />
                </div>
                <div id="login">
                    <table width="100%" border="0" align="left" cellpadding="0" cellspacing="0" id="tab"
                        style="color: #006">
                        <tr>
                            <td height="16" colspan="2" align="center" style="color: #000033; font-size: 14px;">
                                <strong>操作员登录</strong>
                            </td>
                        </tr>
                        <tr>
                            <td colspan="2" height="22">
                                <div class="loginFailed" id="messageInfo">
                                    <noscript>
                                        需要 JavaScript。启用 JavaScript，才能使用系统。
                                    </noscript>
                                </div>
                            </td>
                        </tr>
                        <tr class="row">
                            <td width="24%" height="53" align="right">
                                <label for="UserName" class="text" id="loginUserName">
                                    用户名:</label>
                            </td>
                            <td width="76%" align="left">
                                <span class="ctrl">
                                    <input type="text" id="UserName" name="UserName" maxlength="20" size="20" class="textinput" onkeydown="keyEnter('user')"/>
                                </span>
                            </td>
                        </tr>
                        <tr class="row">
                            <td height="40" align="right">
                                <label for="UserPassword" class="text">
                                    密 码:</label>
                            </td>
                            <td align="left">
                                <span class="ctrl">
                                    <input type="password" id="UserPassword" name="UserPassword" maxlength="20" class="textinput" onkeydown="keyEnter('pwd')"/>
                                </span>
                            </td>
                        </tr>
                        <tr>
                            <td height="29">
                                &nbsp;
                            </td>
                            <td align="left" valign="middle" style="padding-left:5px">
                                <input type="checkbox" name="SaveLogin" style="vertical-align: middle; cursor: pointer;"
                                    id="SaveLogin" />
                                <label style="vertical-align: middle; cursor: pointer;" for="SaveLogin">
                                    记住我的用户名
                                </label>
                            </td>
                        </tr>
                        <tr>
                            <td height="61" align="center" colspan="2">
                                <input type="button" id="ButtonOK" name="ButtonOK" value="  " class="ButtonOK" title="登录" />
                            </td>
                        </tr>
                    </table>
                </div>
            </div>
        </div>
        <div id="footer">
            &nbsp;
        </div>
    </div>
    <input type="hidden" id="RemoteCallUrl" name="RemoteCallUrl" value="ProcessAll" />
    <input type="hidden" id="RemoteCallMethod" name="RemoteCallMethod" value="Hygeia" />
    <input type="hidden" id="AddData" name="AddData" value="auto_login" />
    <input type="hidden" id="auto_login" name="auto_login" value="" />
</body>
</html>
