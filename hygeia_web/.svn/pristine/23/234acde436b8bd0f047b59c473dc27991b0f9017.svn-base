}

span.pagelinks a,span.pagelinks span {
	color: #000;
	border-color: #e4e4e4;
}

span.pagebanner {
	color: #000;
}

div.exportlinks {
	color: #000;
}

.l-button {
	border-color: #e4e4e4;
	background: #fff;
}

.l-button-over,.l-button:hover {
	background: #ededed;
	border-color: #b3b3b3;
}

.l-button-disabled {
