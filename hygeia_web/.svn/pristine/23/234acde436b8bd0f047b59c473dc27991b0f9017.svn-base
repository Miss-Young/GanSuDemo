/*广东人社一体化*/
@CHARSET "UTF-8";

body {
	background-color: #F0F2F7;
	padding-bottom: 10px;
}

.tableEditor, .tableEditor td {
	background-color: #fff !important;
	border-color: #fff !important;
}

.tableEditor tbody tr:first-child td {
	border-color: #fff !important;
}

.required {
	color: #f00;
}

.tdButton {
	text-align: right;
}

td.tdGroup , .tableEditor tbody tr:first-child td.tdGroup, .tableEditor thead th{
	text-align: left;
	padding-left: 12px !important;
	border-left:4px solid #a6ccf7 !important;
}

button.button, input[type="button"].button {
	border-radius: 2px;
	min-width: 70px;
}

.button, .button:hover, .button:focus, .button:active, .button.active, .open .dropdown-toggle.button {
	background-color: #ddb93a;
	border-color: #e4e4e4;
}

.button:hover{
	background-color: #cba007;
	box-shadow: 0 0px 0px rgba(0, 0, 0, .2);
}

button[type="reset"] {
	color: #000;
	background-color: #fff;
	border-color: 1px solid #E4E4E4;
}

textarea:hover,input[type="text"]:hover,input[type="password"]:hover,input[type="datetime"]:hover,input[type="datetime-local"]:hover,input[type="date"]:hover,input[type="month"]:hover,input[type="time"]:hover,input[type="week"]:hover,input[type="number"]:hover,input[type="email"]:hover,input[type="url"]:hover,input[type="search"]:hover,input[type="tel"]:hover,input[type="color"]:hover
	{
	border-color: #ddb93a;
	box-shadow: 0px 0px 0px rgba(0,0,0,0.2);
}

textarea:focus,input[type="text"]:focus,input[type="password"]:focus,input[type="datetime"]:focus,input[type="datetime-local"]:focus,input[type="date"]:focus,input[type="month"]:focus,input[type="time"]:focus,input[type="week"]:focus,input[type="number"]:focus,input[type="email"]:focus,input[type="url"]:focus,input[type="search"]:focus,input[type="tel"]:focus,input[type="color"]:focus
	{
	border-color: #ddb93a;
}

input[readonly],.text[disabled],input[disabled][type="text"],
textarea[disabled],.textarea[disabled],textarea[readonly],.textarea[readonly],
.inputDisabled,.textReadonly,
.input-group>input[readonly][role="spinbutton"] {
	color: #000;
	background: #f1f2f7 !important;
	border: 1px solid #e4e4e4 !important;
	cursor: auto;
	height: 26px;
	line-height: 26px;
}

select[disabled],
.input-group input[readonly], .input-group input[readonly]:focus, 
.input-group>.date[readonly], .input-group>.date.textReadonly, .input-group>.date[disabled],
.select2-container.select2-container-disabled .select2-choice,
.select2-container-multi.select2-container-disabled .select2-choices,
.l-text-disabled .l-text-field{
	background: #f1f2f7 !important;
	border: 1px solid #e4e4e4 !important;
	cursor: auto;
}

.select2-container-disabled .select2-default{
	color: #000 !important;
}

.select-disabled{
	background: #f1f2f7;
	border: 1px solid #e4e4e4;
}

.l-text-combobox > input[readonly]{
	background: #fff !important;
	border: 0 !important;
}

.l-text-disabled{
	border: 1px solid #e4e4e4 !important;
}

.l-text-disabled.l-text-combobox > input[readonly]{
	background: #f1f2f7 !important;
	border: 0 !important;
}

.tableEditor {
	border-radius: 2px;
}

.panelBox {
	background-color: #fff;
	margin-top: 10px !important;
	border-radius: 2px;
	padding: 5px 10px;
}

.panelBoxHeader {
	color: #a3542;
	border-color: #fff;
}

.panelBoxTitle a,.ui-accordion-header a {
	color: #000;
}

.panelBoxTitle > span {
	padding-left: 12px;
	border-left: 4px solid #0081cc;
}

.panelBoxTitle i, .groupboxtitle i {
	display: none;
}


.panelBoxTitle a:hover,.panelBoxTitle a:active,.ui-accordion-header a:hover,.ui-accordion-header a:active
	{
	color: #ff0000;
}

.ui-accordion .ui-state-active.ui-accordion-header a {
	color: #ddb93a;
}

fieldset, legend{
	background: #fff;
	padding: 0;
}

fieldset{
	margin-top: 10px;
	border-radius: 2px;
}

.groupboxtitle {
	color: #000;
	border-bottom: 0;
	padding-left: 12px;
	border-left: 4px solid #0081cc;
}

.ui-tabs .ui-tabs-panel, .tab-content {
	background: #fff;
}

.ui-tabs .ui-tabs-nav {
	border-color: #e4e4e4;
}

.tableEditor {
	border-color: #e4e4e4;
}

.tableEditor tr,.tableEditor td,.tableEditor th {
	border-color: #fff;
}

.tableEditor tbody tr:first-child td {
	border-color: #fff;
}

.tdLabel {
	background: #fff;
