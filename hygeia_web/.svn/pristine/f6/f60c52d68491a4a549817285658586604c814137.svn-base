<script type="text/javascript">
	function S4() {
	   return (((1+Math.random())*0x10000)|0).toString(16).substring(1);
	}
	function guid() {
	   return (S4()+S4()+S4()+S4()+S4()+S4()+S4()+S4());
	}
	$(".date").each(function() {
		var that = $(this);
		that.attr("id", (that.attr("id") || "id") + "-" + guid());
		var length = that.val().length;
		var fmt, tpe
		if(length == 19){
			fmt = "yyyy-MM-dd HH:mm:ss";
			tpe = "datetime";
		} else if(length == 10){
			fmt = "yyyy-MM-dd";
			tpe = "date";
		} else if(length == 8){
			fmt = "yyyyMMdd";
			tpe = "date";
		} else if(length == 7){
			fmt = "yyyy-MM";
			tpe = "month";
		} else if(length == 6){
			fmt = "yyyyMM";
			tpe = "month";
		} else if(length == 4){
			fmt = "yyyy";
			tpe = "year";
		} else {
			fmt = "yyyy-MM-dd";
			tpe = "date";
		}
		laydate.render({
		    elem: "#" + that.attr("id"),
		    format: fmt,
		    type: tpe,
		    isInitValue: false
		});
	});
</script>
