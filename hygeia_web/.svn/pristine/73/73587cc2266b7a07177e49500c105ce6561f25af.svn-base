#============================================================================
# JDBCLogger for development environment 
#============================================================================

#default:false
jdbclogger.enabled=true
#default:0
jdbclogger.minRuntime=0
#default:true
jdbclogger.needCaller=true
#default:10
jdbclogger.maxBatchCount=10
#default:5000
jdbclogger.minRowCount=50000
#default:
jdbclogger.excluded=com\.powersi\.hygeia\.framework\.cache\.*,com\.powersi\.sys\.util\.*,com\.powersi\.sys\.manager\.util\.*,com\.powersi\.sys\.message\.dao\.*
#default:false
jdbclogger.needframework=false